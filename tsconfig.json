{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "noImplicitAny": false,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
}
